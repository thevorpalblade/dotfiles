Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 90
       , commands = [ Run Weather "KSAC" ["-t"," <skyCondition>, <tempF>F","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36
                    , Run MultiCpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %l:%M" "date" 10
		    , Run MPD ["-t",
		              "<artist>: <title> (<album>) <lapsed>/<length> <statei> ",
			      "--", "-P", "|>", "-Z", "||", "-S", "|><|"] 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %mpd% | %multicpu% | %memory% * %swap%    <fc=#ee9a00>%date%</fc> | %KSAC%"
       }

