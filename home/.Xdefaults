URxvt*termName: rxvt

## borderless and no scrollbar
URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderLess: false
URxvt*internalBorder:0

## teh transparency stuff
URxvt*inheritPixmap: true
#URxvt*tintColor: white
URxvt*shading:25

## geometry and font

#URxvt*letterSpace: -3
#URxvt*geometry: 84Ã—24
URxvt*font: xft:terminus:pixelsize=10
#URxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1
#URxvt.font: xft:DejaVu Sans Mono:pixelsize=12
#URxvt.font: -*-proggyclean-*-*-*-*-*-*-*-*-*-*-*-*
#URxvt.font:  6x13
#URxvt.font: xft:Terminess Powerline:style=Regular:pixelsize=10
## change default colors
URxvt*background: #000000
URxvt*foreground: #A8A8A8
#URxvt*foreground: #C0C0C0
URxvt*color0: #000000
URxvt*color1: #A80000
URxvt*color2: #00A800
URxvt*color3: #A85400
